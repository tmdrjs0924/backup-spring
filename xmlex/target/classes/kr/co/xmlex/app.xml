<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Student 클래스의 객체를 생성하여 속성에 값을 주는 xml을 추가 -->
	<bean id="student2" class="kr.co.xmlex.Student">
		<property name="age" value="20"/>
		<property name="name" value="둘리"/>
		<property name="hakbun" value="p1212"/>
	</bean>
	
	<bean id="student3" class="kr.co.xmlex.Student">
		<property name="age" value="30"></property>
		<property name="name" value="희동"></property>
		<property name="hakbun" value="p2323"></property>
	</bean>
	
</beans>